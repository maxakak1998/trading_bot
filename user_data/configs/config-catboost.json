    "exchange": {
        "name": "binance",
        "key": "",
        "secret": "",
        "ccxt_config": {},
        "ccxt_async_config": {},
        "pair_whitelist": [
            "BTC/USDT",
            "ETH/USDT"
        ],
        "pair_blacklist": [
            "BNB/.*",
            ".*DOWN/.*",
            ".*UP/.*",
            ".*BEAR/.*",
            ".*BULL/.*",
            ".*KEEP/.*",
            ".*STAKE/.*"
        ]
    },
    "pairlists": [
        {
            "method": "StaticPairList"
        }
